{
  "name": "versionbits-web",
  "version": "0.0.0",
  "description": "A web viewer for Bitcoin versionbits deployments",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib && babel --presets es2015 src -d lib && browserify lib/main.js -o docs/bundle.js -t [ babelify --presets es2015 ]",
    "debug": "cd docs && beefy ../lib/main.js:bundle.js --live -- -t [ babelify --presets es2015 ]",
    "test": "echo \"Error: no test specified\" && exit 1",
    "source": "rm -rf lib && ln -s src lib",
    "prepublish": "npm run build",
    "publish": "npm run source"
  },
  "keywords": [
    "webcoin",
    "bitcoin",
    "versionbits",
    "bip9",
    "deployments",
    "csv",
    "segwit",
    "viewer",
    "explorer"
  ],
  "author": "Matt Bell <mappum@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "beefy": "^2.1.8",
    "bitcoin-net": "^4.2.4",
    "bitcoin-util": "^2.0.3",
    "blockchain-download": "^1.0.0",
    "blockchain-spv": "^2.0.5",
    "browserify": "^13.0.1",
    "budo": "^9.2.1",
    "hyperx": "^2.0.4",
    "hyperxify": "^2.0.1",
    "level-browserify": "^1.1.0",
    "main-loop": "^3.2.0",
    "object-assign": "^4.1.0",
    "pump": "^1.0.1",
    "setimmediate": "^1.0.4",
    "uglify-js": "^2.7.4",
    "versionbits": "0.0.0",
    "virtual-dom": "^2.1.1",
    "watchify": "^3.7.0",
    "webcoin-bitcoin": "^3.0.1",
    "webcoin-bitcoin-testnet": "^1.0.2"
  },
  "dependencies": {
    "chartist": "^0.9.8",
    "on-object": "^1.0.0",
    "webcoin": "^2.0.1",
    "webcoin-bitcoin": "^3.1.2",
    "webworkify": "^1.4.0",
    "workerstream": "^1.2.1"
  }
}
